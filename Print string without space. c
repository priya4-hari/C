#include<stdio.h>
int main()
{
    char s[50],t[50];
    int i,j;
    printf("enter syring;");
    gets(s);
    i=0;
    j=0;
    while(s[i]!='\0')
    {
        if(s[i]!=' ')
        {
            t[j++]=s[i];
            }
            i++;
            }
      t[j]='\0';
        puts(t);
        }
